{
	"name": "PaliWallet",
	"version": "1.4.4",
	"private": true,
	"scripts": {
		"watch": "./scripts/build.sh watcher watch",
		"clean:ios": "rm -rf ios/build",
		"pod:install": "cd ios/ && pod install && cd ..",
		"clean:android": "rm -rf android/app/build",
		"clean:core": "cd ./../core && rm -rf node_modules && yarn install && yarn build && cd ./../app",
		"clean:node": "rm -rf node_modules && yarn install",
		"clean": "yarn clean:ios && yarn clean:android && yarn clean:core && yarn clean:node && yarn pod:install",
		"lint": "eslint '**/*.js' --ignore-path=.prettierignore",
		"lint:fix": "eslint '**/*.js' --fix --ignore-path=.prettierignore",
		"format": "prettier '**/*.{js,json}' --write",
		"start:ios": "./scripts/build.sh ios debug  --device",
		"start:android": "./scripts/build.sh android debug",
		"build:core": "./scripts/build-core.sh",
		"build:android": "./scripts/build.sh android release",
		"build:ios": "./scripts/build.sh ios release --device",
		"postinstall": "./scripts/postinstall.sh",
		"build:thread": "yarn build:core && ./scripts/build-thread.sh",
		"sourcemaps:android": "rm -rf sourcemaps/index.android.js.map && ./node_modules/.bin/react-native bundle --platform android --entry-file index.js --dev false --reset-cache --bundle-output /tmp/bundle.android.js --assets-dest /tmp/ --sourcemap-output sourcemaps/index.android.js.map",
		"sourcemaps:ios": "rm -rf sourcemaps/index.ios.js.map && ./node_modules/.bin/react-native bundle --platform ios --entry-file index.js --dev false --reset-cache --bundle-output /tmp/bundle.ios.js --assets-dest /tmp/ --sourcemap-output sourcemaps/index.ios.js.map"
	},
	"prettier": {
		"printWidth": 120,
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"prettier --write",
			"eslint --ignore-path=.prettierignore"
		],
		"*.json": [
			"prettier --write"
		]
	},
